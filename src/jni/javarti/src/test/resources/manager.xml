<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<objectModel xsi:schemaLocation="http://standards.ieee.org/IEEE1516-2010 http://standards.ieee.org/downloads/1516/1516.2-2010/IEEE1516-DIF-2010.xsd" xmlns="http://standards.ieee.org/IEEE1516-2010" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelIdentification>
    <name>Manager</name>
    <type>FOM</type>
    <version>1.01</version>
    <modificationDate>2017-06-07</modificationDate>
    <securityClassification>Unclassified</securityClassification>
    <purpose></purpose>
    <applicationDomain>Engineering</applicationDomain>
    <description>Simulationssteuerung</description>
    <useLimitation>NA</useLimitation>
    <poc>
      <pocType>CKC</pocType>
      <pocName>CKC</pocName>
      <pocOrg>oToT</pocOrg>
      <pocTelephone>TODO</pocTelephone>
      <pocEmail>TODO</pocEmail>
    </poc>
    <other>Created by CKC 2017</other>
    <glyph href="SIMDESK_HLA_32x32.png" type="PNG" height="32" width="32" alt="Manager Interaction Module"></glyph>
  </modelIdentification>

  <objects>
    <objectClass>
      <name>HLAobjectRoot</name>
      <objectClass>
        <name>SimLabFederate</name>
        <sharing>PublishSubscribe</sharing>
        <attribute>
          <name>FederateName</name>
          <dataType>HLAunicodeString</dataType>
          <updateType>Static</updateType>
          <updateCondition>NA</updateCondition>
          <ownership>NoTransfer</ownership>
          <sharing>PublishSubscribe</sharing>
          <dimensions/>
          <transportation>HLAreliable</transportation>
          <order>Receive</order>
          <semantics>Name</semantics>
        </attribute>
        <attribute>
          <name>FederateState</name>
          <dataType>FederateStateEnum</dataType>
          <updateType>Conditional</updateType>
          <updateCondition>OnChange</updateCondition>
          <ownership>NoTransfer</ownership>
          <sharing>PublishSubscribe</sharing>
          <dimensions/>
          <transportation>HLAreliable</transportation>
          <order>Receive</order>
          <semantics>Status, in welchem sich Federate gerade befindet</semantics>
        </attribute>
      </objectClass>
    </objectClass>
  </objects>
  
  <interactions>
    <interactionClass>
      <name>HLAinteractionRoot</name>
      <interactionClass>
          <name>Command</name>
          <sharing>PublishSubscribe</sharing>
          <dimensions/>
          <transportation>HLAreliable</transportation>
          <order>Receive</order>
          <semantics>Manager Commands</semantics>
          <parameter>
              <name>Target</name>
              <dataType>HLAunicodeString</dataType>
              <semantics>Der Name von Zielfederate</semantics>
          </parameter>
	      <interactionClass>
	          <name>Start</name>
	          <semantics>Interaction zum  Starten der Simulation</semantics>
	      </interactionClass>
	      <interactionClass>
	          <name>Stop</name>
	          <semantics>Interaction zum  Stoppen der Simulation</semantics>
	      </interactionClass>
	      <interactionClass>
	          <name>Exit</name>
	          <semantics>Interaction zum  Exit der Simulation</semantics>
	      </interactionClass>
	      <interactionClass>
	          <name>LoadData</name>
	          <order>Receive</order>
	          <semantics>Interaction zum Laden von Daten aus der Datei</semantics>
	          <parameter>
	              <name>FileName</name>
	              <dataType>HLAunicodeString</dataType>
	              <semantics>Datei für die Simulation</semantics>
	          </parameter>
	      </interactionClass>
      </interactionClass>
      <interactionClass>
          <name>DataLoaded</name>
          <sharing>PublishSubscribe</sharing>
          <dimensions/>
          <transportation>HLAreliable</transportation>
          <order>Receive</order>
          <semantics>Diese Interaction bestätigt das Laden von Daten</semantics>
          <parameter>
              <name>FederateName</name>
              <dataType>HLAunicodeString</dataType>
              <semantics>Name vom Federate, der erfolgreich die Daten geladen hat</semantics>
          </parameter>
      </interactionClass>
      <interactionClass>
          <name>DataLoadFailure</name>
          <sharing>PublishSubscribe</sharing>
          <dimensions/>
          <transportation>HLAreliable</transportation>
          <order>Receive</order>
          <semantics>Muss vom Federate gesendet werden, der die Daten nicht laden konnte</semantics>
          <parameter>
              <name>FederateName</name>
              <dataType>HLAunicodeString</dataType>
              <semantics>Name vom Federate, der die Daten nicht laden konnte</semantics>
          </parameter>
          <parameter>
              <name>ErrorMessage</name>
              <dataType>HLAunicodeString</dataType>
              <semantics>Die Erklärung, warum die Daten nicht geladen werden könnten</semantics>
          </parameter>
      </interactionClass>
    </interactionClass>
  </interactions>

  <dataTypes>
        <enumeratedDataTypes>
            <enumeratedData>
                <name>FederateStateEnum</name>
                <representation>HLAinteger32BE</representation>
                <semantics>Representiert Federate-Status</semantics>
                <enumerator>
                    <name>Started</name>
                    <value>1</value>
                </enumerator>
                <enumerator>
                    <name>Stopped</name>
                    <value>2</value>
                </enumerator>
            </enumeratedData>
        </enumeratedDataTypes>
  </dataTypes>
</objectModel>
